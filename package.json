{
	"name": "conditional-compile-code-annotation",
	"displayName": "conditional-compile-code-annotation",
	"description": "自动生成条件编译注释",
	"publisher": "chaizhiyuan",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:create.js",
		"onCommand:create.css",
		"onCommand:create.jsx"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
				"command": "create.js",
				"title": "ifdef(js)"
			},
			{
				"command": "create.css",
				"title": "ifdef(css)"
			},
			{
				"command": "create.jsx",
				"title": "ifdef(jsx)"
			}
		],
		"menus": {
			"editor/context": [{
				"command": "create.js",
				"group": "1_ifdef",
				"when": "editorHasSelection"
			}, {
				"command": "create.css",
				"group": "1_ifdef",
				"when": "editorHasSelection"
			}, {
				"command": "create.jsx",
				"group": "1_ifdef",
				"when": "editorHasSelection"
			}]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/826300474/conditional-compile-code-annotation.git"
	}
}